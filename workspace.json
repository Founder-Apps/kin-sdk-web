{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "generators": {},
  "projects": {
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "targets": {
        "lint": { "executor": "@nrwl/linter:eslint", "options": { "lintFilePatterns": ["libs/core/**/*.ts"] } },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": { "jestConfig": "libs/core/jest.config.js", "passWithNoTests": true }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/core",
            "tsConfig": "libs/core/tsconfig.lib.json",
            "packageJson": "libs/core/package.json",
            "main": "libs/core/src/index.ts",
            "assets": [
              "libs/core/*.md",
              { "glob": "**/*", "input": "libs/core/src/generated", "output": "src/generated" }
            ]
          }
        }
      }
    },
    "client": {
      "root": "libs/client",
      "sourceRoot": "libs/client/src",
      "projectType": "library",
      "targets": {
        "lint": { "executor": "@nrwl/linter:eslint", "options": { "lintFilePatterns": ["libs/client/**/*.ts"] } },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/client"],
          "options": { "jestConfig": "libs/client/jest.config.js", "passWithNoTests": true }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/client",
            "tsConfig": "libs/client/tsconfig.lib.json",
            "packageJson": "libs/client/package.json",
            "main": "libs/client/src/index.ts",
            "assets": ["libs/client/*.md"]
          }
        }
      }
    }
  }
}

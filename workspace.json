{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "generators": {},
  "projects": {
    "core": {
      "root": "packages/core",
      "sourceRoot": "packages/core/src",
      "projectType": "library",
      "targets": {
        "lint": { "executor": "@nrwl/linter:eslint", "options": { "lintFilePatterns": ["packages/core/**/*.ts"] } },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/core"],
          "options": { "jestConfig": "packages/core/jest.config.js", "passWithNoTests": true }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/core",
            "tsConfig": "packages/core/tsconfig.lib.json",
            "packageJson": "packages/core/package.json",
            "main": "packages/core/src/index.ts",
            "assets": [
              "packages/core/*.md",
              { "glob": "**/*", "input": "packages/core/src/generated", "output": "src/generated" }
            ]
          }
        }
      }
    },
    "client": {
      "root": "packages/client",
      "sourceRoot": "packages/client/src",
      "projectType": "library",
      "targets": {
        "lint": { "executor": "@nrwl/linter:eslint", "options": { "lintFilePatterns": ["packages/client/**/*.ts"] } },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/client"],
          "options": { "jestConfig": "packages/client/jest.config.js", "passWithNoTests": true }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/client",
            "tsConfig": "packages/client/tsconfig.lib.json",
            "packageJson": "packages/client/package.json",
            "main": "packages/client/src/index.ts",
            "assets": ["packages/client/*.md"]
          }
        }
      }
    }
  }
}
